<Window x:Class="Payment.Delete"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Payment"
        mc:Ignorable="d"
        Title="Delete" Height="300" Width="600">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Label Content="Удалить запись понятный пользователю идентификатор записи" HorizontalAlignment="Center" FontSize="15"  VerticalAlignment="Center" Margin="0 50 0 0"></Label>
        <Button Height="50" Width="130" Content="Отмена" Grid.Row="1" Background="White" FontSize="15"></Button>
        <Button x:Name="btn_delete" Height="50" Width="130" Content="Удалить" Grid.Row="1" Background="White" FontSize="15" HorizontalAlignment="Right" Margin="0 0 60 0"></Button>
    </Grid>
</Window>
